    "@angular/platform-browser": "~7.2.0-rc.0",
    "@angular/platform-browser-dynamic": "~7.2.0-rc.0",
    "@angular/router": "~7.2.0-rc.0",
    "@ng-toolkit/universal": "^1.1.51",
    "bootstrap-scss": "^4.2.1",
    "core-js": "^2.5.4",
    "ngx-quicklink": "^0.0.6",
    "rxjs": "~6.3.3",
    "tslib": "^1.9.0",
    "zone.js": "~0.8.26",
    "@angular/platform-server": "~7.2.0-rc.0",
    "@angular/http": "~7.2.0-rc.0",
    "@nguniversal/module-map-ngfactory-loader": "~7.0.0",
    "webpack-cli": "^3.1.2",
    "ts-loader": "4.2.0",
    "@nguniversal/express-engine": "~7.0.0",
    "@nguniversal/common": "~7.0.0",
    "cors": "~2.8.4",
    "cp-cli": "^1.1.0",
    "aws-serverless-express": "^3.2.0"
